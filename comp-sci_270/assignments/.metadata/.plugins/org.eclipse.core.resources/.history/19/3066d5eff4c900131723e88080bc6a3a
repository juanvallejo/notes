import java.util.Scanner;
public class CLI {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String next = "";
           
        while(!next.equals("bye")) {
            next = in.nextLine();
            System.out.println(">"+next);
            if(next.equals("bye")) {
                System.out.println("Exiting...");
            }
        }
    }
}
