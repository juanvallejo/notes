import java.util.Scanner;
public class CLI {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String next = in.nextLine();
                
        while(next != "bye") {
            next = in.nextLine();
            if(next == "bye") {
                System.out.println("shit");
            }
        }
    }
}
